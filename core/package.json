{
  "name": "@google/adk",
  "version": "0.1.0",
  "description": "Google ADK JS",
  "author": "Google",
  "license": "Apache-2.0",
  "repository": {
    "type": "git",
    "url": "https://github.com/google/adk-js"
  },
  "files": [
    "package.json",
    "README.md",
    "LICENSE",
    "dist"
  ],
  "type": "module",
  "typings": "./dist/types/index.d.ts",
  "main": "./dist/cjs/index.js",
  "module": "./dist/esm/index.js",
  "browser": "./dist/web/index_web.js",
  "exports": {
    ".": {
      "types": "./dist/types/index.d.ts",
      "import": "./dist/esm/index.js",
      "require": "./dist/cjs/index.js",
      "default": "./dist/esm/index.js"
    }
  },
  "scripts": {
    "clean": "rm -rf dist",
    "clean:all": "npm run clean && rm -rf node_modules",
    "build": "tsc --emitDeclarationOnly && node ./build.js",
    "build:bundle": "tsc --emitDeclarationOnly && node ./build.js --bundle",
    "build:watch": "concurrently \"tsc --noEmit --watch --preserveWatchOutput\" \"node ./build.js --watch\"",
    "test": "vitest",
    "lint": "eslint 'src/**/*.ts'",
    "format": "prettier --write 'src/**/*.ts'",
    "prepublishOnly": "npm run build:bundle"
  },
  "devDependencies": {
    "openapi-types": "^12.1.3"
  },
  "peerDependencies": {
    "@google/genai": "1.14.0",
    "@modelcontextprotocol/sdk": "1.17.5",
    "@opentelemetry/api": "1.9.0",
    "zod": "3.25.76"
  }
}
