{
  "name": "@google/adk_cli",
  "version": "0.1.0",
  "description": "Google ADK JS CLI",
  "author": "",
  "license": "Apache-2.0",
  "type": "commonjs",
  "main": "./dist/cli/cli.mjs",
  "bin": {
    "adk": "./dist/cli/cli.cjs"
  },
  "exports": {
    ".": {
      "types": "./dist/cli/cli.d.ts",
      "require": "./dist/cli/cli.cjs"
    }
  },
  "scripts": {
    "clean": "rm -rf dist",
    "clean:all": "npm run clean && rm -rf node_modules",
    "build": "npm run build:types && npm run build:node_cjs && npm run build:copy_assets",
    "build:node_cjs": "esbuild ./src/cli/cli.ts --outfile=dist/cli/cli.cjs --format=cjs --bundle --platform=node --target=node10.4 --packages=external",
    "build:copy_assets": "cp -r ./src/browser ./dist/browser",
    "build:types": "tsc --emitDeclarationOnly",
    "test": "vitest",
    "lint": "eslint 'src/**/*.ts'",
    "format": "prettier --write 'src/**/*.ts'",
    "prepublishOnly": "npm run build"
  },
  "devDependencies": {
    "@types/body-parser": "^1.19.6",
    "@types/express": "^4.17.21",
    "@types/node": "^20.12.7",
    "@typescript-eslint/eslint-plugin": "^7.7.1",
    "@typescript-eslint/parser": "^7.7.1",
    "esbuild": "^0.25.9",
    "eslint": "^8.57.0",
    "eslint-config-prettier": "^9.1.0",
    "prettier": "^3.2.5",
    "tslib": "^2.6.2",
    "typescript": "^5.9.2",
    "vitest": "^3.2.4"
  },
  "dependencies": {
    "@google/adk": "../core",
    "body-parser": "^2.2.0",
    "commander": "^14.0.0",
    "express": "^4.21.2"
  }
}
